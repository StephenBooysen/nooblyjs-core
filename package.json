{
  "name": "noobly-core",
  "version": "1.3.1",
  "description": "A powerful set of modular Node.js backend services with singleton pattern - caching, logging, data serving, filing, measuring, notifying, queueing, scheduling, searching, workflow, and working services.",
  "keywords": [
    "nodejs",
    "backend",
    "services",
    "microservices",
    "cache",
    "logging",
    "queue",
    "workflow",
    "express"
  ],
  "files": [
    "src",
    "README.md",
    "package.json",
    "index.js"
  ],
  "license": "ISC",
  "author": "NooblyJS",
  "type": "commonjs",
  "main": "index.js",
  "engines": {
    "node": ">=12.11.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nooblyjs/nooblyjs-core.git"
  },
  "scripts": {
    "clean": "rimraf dist/*; rimraf jsdocs/*;",
    "dev:web": "npm run clean; nodemon --watch ./src ./app.js",
    "test": "jest",
    "test-load": "node ./tests-load/",
    "build": "jsdoc -c jsdoc.json; npm version minor; babel src -d dist; npm pack --pack-destination='package'"
  },
  "jest": {
    "modulePaths": [
      "./"
    ],
    "transform": {},
    "forceExit": true,
    "detectOpenHandles": true
  },
  "devDependencies": {
    "@babel/cli": "^7.28.0",
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "gts": "^6.0.2",
    "jest": "^30.0.3",
    "jsdoc": "^4.0.4",
    "supertest": "^7.1.4"
  },
  "dependencies": {
    "@anthropic-ai/claude-code": "^1.0.81",
    "@anthropic-ai/sdk": "^0.60.0",
    "@google-cloud/storage": "^7.17.0",
    "@google/gemini-cli": "^0.1.22",
    "aws-sdk": "^2.1620.0",
    "body-parser": "^1.20.2",
    "dotenv": "^17.2.1",
    "express": "^4.19.2",
    "ftp": "^0.3.10",
    "ioredis": "^5.6.1",
    "ioredis-mock": "^8.9.0",
    "memjs": "^1.3.0",
    "mongodb": "^6.19.0",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.10",
    "openai": "^5.16.0",
    "redis": "^5.5.6",
    "rimraf": "^6.0.1",
    "simple-git": "^3.28.0",
    "uuid": "^11.1.0"
  }
}
