{
  "source": {
    "include": ["./src"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs/)"
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./jsdoc/",
    "recurse": true,
    "verbose": true
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}